
@{
    ViewBag.Title = "Cargar Arcchivo a descomprimir";
}

<h2>Carga de archivo</h2>
<div class="jumbotron">
    <h2 class="display-3">Carga dearchivo</h2>
    <hr class="my-4">
    <p class="lead">
        @using (Html.BeginForm("LecturaComprimido", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
         {
            <p> Subir archivo</p>
            <input type="file" name="HUFF">
            <hr />
            <input type="submit" value="Subir" class="btn btn-outline-success" />
         }
    </p>
    <h6>@ViewBag.Msg</h6>
</div>
