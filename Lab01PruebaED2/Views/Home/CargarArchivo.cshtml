
@{
    ViewBag.Title = "Cargar Archivo";
}

<h2>CargarArchivo</h2>
<div class="jumbotron">
    <h2 class="display-3">Carga dearchivo</h2>
    <hr class="my-4">
        <p class="lead">
            @using (Html.BeginForm("LeerArchivo", "Home", FormMethod.Post, new { enctype = "multipart/form-data" }))
                {
                    <p>Subir archivo</p>
                    <input type="file" name="AComprimir" />
                    <hr />
                    <input type="submit" value="Subir" class="btn btn-outline-success" />
                }
        </p>
        <h6>@ViewBag.Msg</h6>
</div>
